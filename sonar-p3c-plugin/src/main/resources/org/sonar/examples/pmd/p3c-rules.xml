<rules>
    <rule>
        <key>ClassNamingShouldBeCamelRule</key>
        <name>ClassNamingShouldBeCamelRule</name>
        <priority>MAJOR</priority>
        <configKey>rulesets/java/ali-naming.xml/ClassNamingShouldBeCamelRule</configKey>
        <description>class name should be camel</description>
    </rule>
    <rule>
        <key>AbstractClassShouldStartWithAbstractNamingRule</key>
        <name>AbstractClassShouldStartWithAbstractNamingRule</name>
        <priority>CRITICAL</priority>
        <configKey>rulesets/java/ali-naming.xml/AbstractClassShouldStartWithAbstractNamingRule</configKey>
        <description>class name should start with abstract</description>
    </rule>
</rules>
